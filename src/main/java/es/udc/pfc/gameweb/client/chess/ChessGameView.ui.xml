<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:chess="urn:import:es.udc.pfc.gameweb.client.chess">
	<ui:with field="i18n" type="es.udc.pfc.gameweb.client.chess.ChessMessages" />
	<g:HTMLPanel styleName="row">
		<div class="span8">
			<chess:ChessBoardWidget ui:field="boardWidget" />
			<g:Label ui:field="response" horizontalAlignment="ALIGN_CENTER" />
		</div>
		<div class="span4">
			<g:VerticalPanel ui:field="roomMessages" />
			<g:Label text="{i18n.labelMessage}:" horizontalAlignment="ALIGN_RIGHT" />
			<g:TextBox ui:field="chatBox" />
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 