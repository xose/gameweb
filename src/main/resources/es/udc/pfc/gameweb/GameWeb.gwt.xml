<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="GameWeb">
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.activity.Activity"/>
  <inherits name="com.google.gwt.place.Place"/>
  <inherits name="com.google.gwt.inject.Inject"/>
  <inherits name="com.google.gwt.logging.Logging" />
  
  <inherits name="com.google.gwt.user.theme.clean.Clean"/>
  
  <inherits name='com.calclab.emite.core.EmiteCore' />
  <inherits name='com.calclab.emite.im.EmiteIM' />
  <inherits name='com.calclab.emite.reconnect.EmiteReconnect' />
  <inherits name='com.calclab.emite.xep.muc.EmiteMUC' />
  
  <inherits name='es.udc.pfc.gamelib.GameLib' />
  
  <set-configuration-property name="UiBinder.useSafeHtmlTemplates" value="true" />
  
  <set-property name="gwt.logging.logLevel" value="INFO"/>
  <set-property name="gwt.logging.enabled" value="TRUE"/>
  <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>
  <set-property name="gwt.logging.popupHandler" value="DISABLED"/>
  
  <entry-point class='es.udc.pfc.gameweb.client.GameWeb' />
  
  <set-property name="user.agent" value="safari"/>
</module>